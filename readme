# Arrays can be like
var names string[] = [ "harry", "howard" }
# Maps are like
var map(string user, int number) users = [
    { user="User", number=1 },
    { user="User", number=2 },
]
# structs are like
var struct name = {
    name: type
}
# Enums are like
var enum name = {
    thing,
}

